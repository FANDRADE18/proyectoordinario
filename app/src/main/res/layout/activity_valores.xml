<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/valores"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".valores">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/vision"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#FFFFFF"
        tools:context=".vision">

        <Button
            android:id="@+id/btncontactanos4"
            android:layout_width="141dp"
            android:layout_height="50dp"
            android:layout_marginTop="48dp"
            android:layout_marginEnd="4dp"
            android:background="#3362F3"
            android:text="CONTACTANOS"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.597"
            app:layout_constraintStart_toEndOf="@+id/btnregresar5"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <Button
            android:id="@+id/btnregresar5"
            android:layout_width="141dp"
            android:layout_height="50dp"
            android:layout_marginStart="32dp"
            android:layout_marginTop="48dp"
            android:background="#3362F3"
            android:text="REGRESAR"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/textView4" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="331dp"
            android:layout_height="231dp"
            android:layout_marginTop="64dp"
            android:text="Cercanía al cliente, Profecionalidad en todas nuestras soluciones, Garantía, Respeto y Empatía, Innovación, Creatividad e Implementación de ideas creativas en la propia compañia, Compromiso con nuestros clientes, Responsabilidad."
            android:textColor="#8768DA"
            android:textSize="18sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/btnmision5" />

        <ImageView
            android:id="@+id/btnmision5"
            android:layout_width="383dp"
            android:layout_height="137dp"
            android:layout_marginTop="64dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/logo6"
            app:srcCompat="@drawable/valores" />

        <ImageView
            android:id="@+id/logo6"
            android:layout_width="121dp"
            android:layout_height="95dp"
            android:layout_marginTop="4dp"
            android:foreground="@drawable/logocodeline"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.058"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <ImageView
        android:id="@+id/btnsalir3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textusuario5"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/exit" />

    <TextView
        android:id="@+id/textusuario5"
        android:layout_width="187dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:text="USUARIO"
        android:textAlignment="textStart"
        android:textColor="#3362F3"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/logo4"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/logo4"
        android:layout_width="121dp"
        android:layout_height="95dp"
        android:layout_marginTop="4dp"
        android:foreground="@drawable/logocodeline"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>